{
    "name": "pinweb/qnlive",
    "description": "Qiniu Live for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "atan",
            "email": "atan@pinweb.io",
            "homepage": "http://www.pinweb.io"
        }
    ],
    "homepage": "https://github.com/pinweb/qnlive",
    "keywords": [
        "Laravel",
        "Qnlive"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pinweb\\Qnlive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pinweb\\Qnlive\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pinweb\\Qnlive\\QnliveServiceProvider"
            ],
            "aliases": {
                "Qnlive": "Pinweb\\Qnlive\\Facades\\Qnlive"
            }
        }
    }
}
